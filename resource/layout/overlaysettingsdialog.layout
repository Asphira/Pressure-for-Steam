"resource/layout/overlaydialogsettings.layout"
{
	controls
	{
	}

	styles
	{
	
		Page {
			inset="0 10 0 0"
			font-family=basefont
			font-style="none"
			
			bgcolor=none
			
			render {
				0="gradient(x0,y1-6,x1,y1, none, orange)"
			}
		}
	}
	
	layout
	{
		place [!$OSX]  { 
			control="frame_minimize,frame_close" 
			align=right 
			margin-top=-2 
			margin-right=6 
			spacing=-9 
		}
		place { control="Page" width=max height=max margin-left=0 margin-right=0 margin-top=20 margin-bottom=40 }

		region { name="right" align=right height=max width=192 margin=0 margin-top=0 }
		place { control="OKButton,CancelButton" region=right align=bottom width=84 height=28 spacing=8 margin-right=8 margin-bottom=8}
	}
}
